{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    // "mocha": true,
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/prefer-stateless-function": "off",
    "no-console": "off",
    "react/prefer-es6-class": "off",
    "react/prop-types":"off",
    "semi":"warn",
    "arrow-body-style":"off",
    "no-trailing-spaces":"warn",
    "spaced-comment":"warn",
    "comma-dangle":"warn",
    "no-multiple-empty-lines":"warn",
    "quotes":"warn",
    "max-len":"warn"
  },
}
